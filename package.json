{
  "name": "fivetwoone",
  "version": "0.0.1",
  "private": true,
  "description": "a helpful app for choosing where to eat",
  "engines": {
    "node": "8.9.0"
  },
  "scripts": {
    "start": "node server",
    "dev": "clear && nodemon server --ignore react-ui/",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "express": "^4.16.2",
    "mongodb": "^3.0.2",
    "socket.io": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blanebaumgartner/FiveTwoOne"
  },
  "devDependencies": {
    "nodemon": "^1.17.1"
  }
}
